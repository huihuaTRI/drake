<?xml version="1.0" ?>
<robot name="couch_rough">
  <!--
  A simple box to represent a couch
  -->

  <link name="couch">
    <!-- Origin: Bottom Center -->
    <visual>
      <origin xyz="0.2 0.2 0.1"/>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" />
      <mass value="26.1" />
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <geometry>
        <box size="0.1 0.1 0.1" />
      </geometry>
      <material name="yellow" />
    </collision>
  </link>
</robot>
